{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["./src"],
    "includePattern": ".+\\.js(doc|x)?$"
  },
  "opts": {
    "encoding": "utf8",
    "destination": "../../website/docs",
    "recurse": true,
    "template": "./node_modules/tui-jsdoc-template",
    "readme": "./README.md"
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  }
}
